class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        # cannot create new array
        # https://ithelp.ithome.com.tw/articles/10270237
        i = 0
        j = 1
        
        while j < len(nums):
            if nums[j] == nums[i]:
                j+=1
            else:
                i+=1
                nums[i] = nums[j]
                j+=1
        return i+1
